{
  "openapi": "3.0.3",
  "servers": [
    {
      "url": "https://api.restpoint.io/api",
      "description": "RestPoint.io default api host address"
    }
  ],
  "info": {
    "version": "1.0.0",
    "title": "DA219A-fullstack-group5",
    "description": "Dining Dashboard"
  },
  "paths": {
    "/allTables": {
      "get": {
        "description": "Get all tables to render restaurant view.",
        "operationId": "get-allTables",
        "tags": [
          "allTables"
        ],
        "responses": {
          "200": {
            "description": "Return all tables and which ones are occupied.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AllTables"
                }
              }
            }
          }
        }
      }
    },
    "/currentTables": {
      "get": {
        "description": "Get all currently occupied tables to render restaurant view accordingly.",
        "operationId": "get-currentTables",
        "tags": [
          "currentTables"
        ],
        "responses": {
          "200": {
            "description": "Return array of all occupied tables at the current date and time.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CurrentTable"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/currentTables/{id}": {
      "get": {
        "description": "Access the current instance of a table.",
        "operationId": "get-currentTables-by-id",
        "tags": [
          "currentTables"
        ],
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Return data about table.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CurrentTable"
                  }
                }
              }
            }
          },
          "404": {
            "description": "Could not find resource",
            "$ref": "#/components/responses/NotFound"
          }
        }
      },
      "put": {
        "description": "",
        "operationId": "put-currentTables-by-id",
        "tags": [
          "currentTables"
        ],
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CurrentTable"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CurrentTable"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/menu": {
      "get": {
        "description": "Get all items on the menu.",
        "operationId": "get-menu",
        "tags": [
          "menu"
        ],
        "responses": {
          "200": {
            "description": "Return an array of all items on the menu.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Menuitem"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/menu/categories": {
      "get": {
        "description": "Get the name and id of all categories on the menu in db in order to render the menu element.",
        "operationId": "get-categories-menu",
        "tags": [
          "menu/categories"
        ],
        "responses": {
          "200": {
            "description": "Return the name and id of all categories in the menu.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Category"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/orders": {
      "get": {
        "description": "Access orders by tableInstanceID.",
        "operationId": "get-orders-by-id",
        "tags": [
          "orders"
        ],
        "responses": {
          "200": {
            "description": "Return order item for the provided tableInstanceID.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Order"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Client issued a bad request",
            "$ref": "#/components/responses/BadRequest"
          },
          "404": {
            "description": "Could not find resource",
            "$ref": "#/components/responses/NotFound"
          }
        }
      },
      "put": {
        "description": "",
        "operationId": "put-orders-by-id",
        "tags": [
          "orders"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Order"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Order"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "x-api-key",
        "description": "Use in Authorize below - b93acfd52c6e466d88390957a697b754"
      },
      "ApiKeyAuth2": {
        "type": "apiKey",
        "in": "header",
        "name": "x-project",
        "description": "Use in Authorize below - 79bff897-19e9-4ad4-a890-e210b8f1451e"
      }
    },
    "schemas": {
      "Menuitem": {
        "type": "object",
        "required": [
          "id"
        ],
        "properties": {
          "id": {
            "type": "string",
            "format": "string"
          },
          "name": {
            "type": "string",
            "format": "string"
          },
          "category": {
            "type": "string",
            "format": "enum"
          },
          "price": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "AllTables": {
        "type": "object",
        "required": [
          "numberOfTables",
          "currentlyOccupiedTables"
        ],
        "properties": {
          "numberOfTables": {
            "type": "string",
            "format": "string"
          },
          "currentlyOccupiedTables": {
            "$ref": "#/components/schemas/CurrentlyOccupiedTables"
          }
        }
      },
      "Category": {
        "type": "object",
        "required": [
          "id"
        ],
        "properties": {
          "id": {
            "type": "string",
            "format": "string"
          },
          "categoryName": {
            "type": "string",
            "format": "string"
          }
        }
      },
      "CurrentTable": {
        "type": "object",
        "required": [
          "tableInstanceID"
        ],
        "properties": {
          "tableInstanceID": {
            "type": "string",
            "format": "string"
          },
          "tableID": {
            "type": "string",
            "format": "string"
          },
          "waiterID": {
            "type": "string",
            "format": "string"
          },
          "startTime": {
            "type": "string",
            "format": "string"
          },
          "totalPrice": {
            "type": "integer",
            "format": "int32"
          },
          "checkOutTime": {
            "type": "integer",
            "format": "string"
          },
          "orderID": {
            "type": "string",
            "format": "string"
          }
        }
      },
      "Order": {
        "type": "object",
        "required": [
          "orderID",
          "tableInstanceID",
          "tableID"
        ],
        "properties": {
          "orderID": {
            "type": "string",
            "format": "string"
          },
          "tableInstanceID": {
            "type": "string",
            "format": "string"
          },
          "tableID": {
            "type": "string",
            "format": "string"
          },
          "items": {
            "$ref": "#/components/schemas/Items"
          },
          "orderTotal": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "DefaultError": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "format": "string"
          },
          "description": {
            "type": "string",
            "format": "string"
          }
        }
      },
      "Items": {
        "type": "object",
        "properties": {
          "itemID": {
            "type": "string",
            "format": "string"
          },
          "category": {
            "type": "string",
            "format": "string"
          },
          "itemName": {
            "type": "string",
            "format": "string"
          },
          "price": {
            "type": "string",
            "format": "string"
          },
          "quantity": {
            "type": "string",
            "format": "string"
          },
          "comment": {
            "type": "string",
            "format": "string"
          },
          "completed": {
            "type": "string",
            "format": "string"
          }
        }
      },
      "MenuCategories": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "format": "string"
          }
        }
      },
      "CurrentlyOccupiedTables": {
        "type": "object",
        "properties": {
          "tableID": {
            "type": "string",
            "format": "string"
          }
        }
      }
    },
    "responses": {
      "BadRequest": {
        "description": "Client issued a bad request",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/DefaultError"
            }
          }
        }
      },
      "Unauthorised": {
        "description": "Unauthorised request",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/DefaultError"
            }
          }
        }
      },
      "NotFound": {
        "description": "Could not find resource",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/DefaultError"
            }
          }
        }
      }
    }
  },
  "security": [
    {
      "ApiKeyAuth": [],
      "ApiKeyAuth2": []
    }
  ]
}